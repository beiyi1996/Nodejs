    "aws-sdk": "^2.561.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.1",
    "connect-mongo": "^3.1.2",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.4.0",
